ціль а = 2;

змінна б = 3;

дія додати(а: ціле, б: ціле): ціле {
  вернути а + б;
}

структура Координати {
  в: дійсне;
  ш: дійсне;
}

композиція Людина {
  властивість імʼя: текст;
  
  дія привітатись() {
    друк(я.імʼя + " каже привіт!");
  }
}

секція двигуни {
  структура РеактивнийДвигун {
    потужність: ціле;
  }
  
  структура ІоннийДвигун {
    потужність: ціле;
  }
}