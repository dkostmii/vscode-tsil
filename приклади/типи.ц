ціль а = 2;

ціль а: ц64 = -3383;

ціль а: ц32 = 22;

ціль а: ц8 = 127;

ціль а = 3.14;

ціль а: д32 = 2.12;

ціль а: позитивне = 2;

ціль а: п64 = -3383;

ціль а: п32 = 22;

ціль а: п8 = 256;

ціль добре = так;
ціль погано = ні;

ціль п = "привіт";

ціль с = сі"привіт";

ціль числа: ціле[4];
числа[1] = 227;
ціль перше = числа[1]; // 227

ціль байти = виділити<п8>(256);

дія сума(а: ціле, б: ціле): ціле {
  вернути а + б;          
}

ціль д: (ціле, ціле) -> ціле = привіт;